{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{css,scss,js,png,gif,ico,jpg,svg}",
      "/*.json"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "serve": "api"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ]
}
